<?xml version="1.0" encoding="UTF-8"?>

<!--<?import com.gluonhq.charm.glisten.control.Avatar?>-->

<?import com.jfoenix.controls.JFXCheckBox?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.shape.Circle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<?import org.kordamp.ikonli.javafx.FontIcon?>

<VBox fx:id="itemPane" prefHeight="72.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <AnchorPane VBox.vgrow="ALWAYS">
            <children>
                <VBox alignment="CENTER_LEFT" prefHeight="200.0" prefWidth="100.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="72.0" AnchorPane.rightAnchor="56.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <Text fx:id="txtTitle" strokeType="OUTSIDE" strokeWidth="0.0" text="Two -line item">
                            <font>
                                <Font size="16.0" />
                            </font>
                        </Text>
                        <Text fx:id="txtDesc" fill="#393939" strokeType="OUTSIDE" strokeWidth="0.0" text="Secondary text" />
                    </children>
                </VBox>
                <HBox alignment="CENTER" prefWidth="72.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <StackPane styleClass="custom-jfx-list-view-icon-container">
                            <Circle opacity="0.2" radius="20.0" stroke="BLACK" strokeType="INSIDE" strokeWidth="0.0" />
                            <FontIcon fx:id="icoSound" iconColor="RED" iconLiteral="fas-volume-down" iconSize="24" styleClass="custom-jfx-list-view-icon" />
                        </StackPane>
                    </children>
                </HBox>
                <HBox alignment="CENTER" prefWidth="56.0" AnchorPane.bottomAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                    <children>
                        <JFXCheckBox fx:id="cbEnable" alignment="CENTER" contentDisplay="GRAPHIC_ONLY" graphicTextGap="0.0" text=" " wrapText="true">
                            <opaqueInsets>
                                <Insets />
                            </opaqueInsets>
                        </JFXCheckBox>
                    </children>
                </HBox>
            </children>
        </AnchorPane>
    </children>
</VBox>
